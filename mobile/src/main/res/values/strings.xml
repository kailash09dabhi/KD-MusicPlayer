  <string name="tab_Albums">Albums</string>
  <string name="tab_Artists">Artists</string>
  <string name="tab_Genres">Genres</string>
  <string name="tab_My_Files">My Files</string>
  <string name="tab_Playlists">Playlists</string>
  <string name="tab_Settings">Settings</string>
  <string name="tab_Songs">Songs</string>
  <!-- MediaStatTable MusicPlayer -->
  <string name="albumsongseparator">\n</string>
  <!-- used in various places to indicate there is one genre for a given artist or album -->
  <string name="onesong">1 genre</string>
  <!-- used in various places to indicate there is some number other than one songs for a given artist or album. -->
  <plurals xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" name="Nsongs">
    <item quantity="other"><xliff:g id="count">%d</xliff:g> songs</item>
  </plurals>
  <!-- shows how many songs on the album are by the selected artist, out of how many total, if those two numbers are different -->
  <plurals xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" name="Nsongscomp">
    <item quantity="other"><xliff:g id="count_for_artist">%2$d</xliff:g> of <xliff:g id="total_count">%1$d</xliff:g> songs</item>
  </plurals>
  <!-- Used in artists list view, indicates how many albums exist for a given artist. -->
  <plurals xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" name="Nalbums">
    <!-- number of albums is one -->
    <item quantity="one">1 album</item>
    <!-- number of albums is not equal to one -->
    <item quantity="other"><xliff:g id="count">%d</xliff:g> albums</item>
  </plurals>
  <!-- Default name of artist that doesn't have a name in the metadata -->
  <string name="unknown_artist_name">Unknown artist</string>
  <!-- The fast scroll thumb will show one character from this string depending on the position in the list -->
  <string name="fast_scroll_alphabet">\u0020ABCDEFGHIJKLMNOPQRSTUVWXYZ</string>
  <!-- MediaStatTable -->
  <string name="mp.music.default_duration">00:00</string>
  <string name="message_empty_due_to_duration_filter">As you have set the duration filter of %d
    seconds so no music below the duration threshold is visible!</string>
  <string name="error_message_couldnt_fetch_music">We are sorry! But we couldnt fetch the music!</string>
  <string name="apply">Apply</string>
</resources>
